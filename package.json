{
  "name": "pixel-papercraft-generator-builder",
  "version": "1.0.0",
  "description": "Papercraft generator builder for Pixel Papercraft",
  "author": "Pixel Papercraft",
  "license": "ISC",
  "type": "commonjs",
  "engines": {
    "node": ">=18 <19",
    "npm": ">=9 <10"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run res:build && webpack serve",
    "res:build": "rescript build",
    "res:watch": "rescript build -w",
    "build": "npm run res:build && NODE_ENV=production webpack",
    "make-block-textures": "node ./src/tools/minecraft-block/MinecraftBlockMakeTextures.bs.js",
    "make-textures": "node ./src/tools/make-textures/MakeTextures.bs.js"
  },
  "dependencies": {
    "har-validator": "^5.1.5",
    "jspdf": "2.5.1",
    "marked": "4.2.12",
    "nbt": "^0.8.1",
    "print-html-element": "0.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "request": "^2.88.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.21.3",
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@rescript/core": "^0.2.0",
    "@rescript/react": "0.10.3",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.14",
    "babel-loader": "9.1.2",
    "css-loader": "6.7.3",
    "html-webpack-plugin": "5.5.0",
    "imageinfo": "1.0.4",
    "jimp": "^0.22.10",
    "merge-img": "2.1.3",
    "postcss": "^8.4.33",
    "postcss-loader": "7.1.0",
    "rescript": "^10.1.3",
    "rescript-jzon": "^1.3.0",
    "style-loader": "3.3.2",
    "tailwindcss": "3.2.7",
    "webpack": "5.76.2",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.13.0"
  }
}
